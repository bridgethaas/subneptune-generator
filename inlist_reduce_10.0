
&star_job
      show_log_description_at_start = .true. 

      save_model_when_terminate = .true.
      save_model_filename = "pre_reduce_.mod"

      load_saved_model = .true.
      saved_model_name = "initial_planet.mod"

      change_initial_net = .true.      
      new_net_name = 'basic.net'

      set_initial_model_number = .true.
      initial_model_number = 0

      !set_initial_dt = .true.
      !years_for_initial_dt = 1d3 ! in years

      set_initial_age = .true.
      initial_age = 0

      eos_file_prefix = 'mesa'
      kappa_file_prefix = 'gs98'
      kappa_lowT_prefix = 'lowT_Freedman11'

      set_tau_factor = .true. ! change tau_factor without reconverging.
      set_to_this_tau_factor = 100
      
      change_lnPgas_flag = .true.
      new_lnPgas_flag = .true.

      !relax_initial_mass = .true.
      !new_mass = 2.50d-4 !5 Neptune Mass
      !lg_max_abs_mdot = -100

      relax_mass = .true.
      relax_initial_mass = .true.
      new_mass = 0.000900361954555 !! Sets it to 30x mp
      lg_max_abs_mdot = 3.0d-10 !(1/10000 earths per year)
    steps_to_take_before_terminate = 25000 
/ ! end of star_job namelist

&controls 

      
      max_age = 1d6
      !max_years_for_timestep = 1d5

      max_number_backups = 100
      max_number_retries = 500
      max_model_number = 1000
      
      mesh_delta_coeff = 0.5
      
      use_Type2_opacities = .false.

      mixing_length_alpha = 1.89d0
      MLT_option = 'Henyey'

      photo_interval = 50
      profile_interval = 100
      history_interval = 100
      terminal_interval = 50
      write_header_frequency = 100
      
      T_mix_limit = 0
      !star_history_name = "pre_reduce_.mod"
/ ! end of controls namelist

&pgstar
/ ! end of pgstar namelist
