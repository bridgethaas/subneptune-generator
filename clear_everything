#!/bin/bash

#Removes Inlists
rm inlist_comp_*
rm inlist_corer_*
rm inlist_corel_*
rm inlist_reduce_*
rm inlist_remove_heating_*
rm inlist_corem_*
rm inlist_irrad_*
rm inlist_evolve_*
rm inlist_heating_*
rm inlist_pre_reduce_*
rm inlist_pre_core_*
rm inlist_remove_cooling_*
rm inlist_cooling_*

#Get rid of .mod files
rm comp_*
rm corer_*
rm corel_*
rm reduce_*
rm remove_*
rm corem_*
rm heating_*
rm pre_reduce*
rm pre_core_*
rm irrad*
rm evolve*
    
#Remove final profiles
rm profile_irrad*
rm profile_remove_heating*
rm profile_remove_cooling*
rm profile_evolve*

#Clear photos directory
cd photos
rm x*

#Clear LOGS directory
cd ..
cd LOGS

rm hist_comp_*
rm hist_corel_*
rm hist_reduce_*
rm hist_remove_*
rm hist_corem_*
rm hist_heating_*
rm hist_irrad_*
rm hist_evolve_*
rm hist_pre_core_*
rm hist_pre_reduce_*

rm profile*
